<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <comment>
	This configuration provides locale-dependent names for labels in
	a (graphical) user interface. The separator ";" can be used to
	separate actual labels from associated tool tips.
  </comment>
  
  <!-- BaseFrame -->
  
  <entry key="ABOUT_THE_PROGRAM">About %s</entry>
  <entry key="DEFAULT_TOOL_BAR_TITLE">Tools</entry>
  <entry key="LICENSE_OF_THE_PROGRAM">%s - License</entry>
  <entry key="ONLINE_HELP_FOR_THE_PROGRAM">%s - Online Help</entry>
  
  <!-- CSVImporter -->
  
  <entry key="COULD_NOT_IDENTIFY_COLUMNS">%d data columns from the given input file could not be automatically assigned to expected columns. Please manually assign these columns here.</entry>
  <entry key="COLUMN_ASSIGNMENT">Column assignment</entry>
  
  <!-- CSVReaderColumnChooser -->
  
  <entry key="REQUIRED_COLUMNS">Required columns </entry>
  <entry key="OPTIONAL_COLUMNS">Optional columns </entry>
  <entry key="ASSIGN_THESE_COLUMNS">Please assign the following columns:</entry>
  
  <!-- CSVReaderOptionPanel -->
  
  <entry key="COLUMN">Column </entry>
  <entry key="CSV_OPTIONS">CSV Options</entry>
  <entry key="FILE_PREVIEW">File preview</entry>
  <entry key="FILE_CONTAINS_HEADERS">File contains headers</entry>
  <entry key="INDICATE_IF_COLUMN_DESCRIPTIONS_ARE_GIVEN">Indicate whether a file contains column descriptions or not.</entry>
  <entry key="SEPARATOR_CHAR">Separator char </entry>
  <entry key="SEPARATOR_CHAR_TOOLTIP">The separator character that is used to split the columns.</entry>
  <entry key="TREAT_CONSECUTIVE_SEPARATORS_AS_ONE">Treat consecutive separators as one</entry>
  <entry key="TREAT_CONSECUTIVE_SEPARATORS_AS_ONE_TOOLTIP">Indicate whether multiple consecutive separators should be treated as one.</entry>
  <entry key="SKIP_THIS_NUMBER_OF_LEADING_LINES">Skip that much lines at the beginning of the file.</entry>
  <entry key="SKIP_LINES">Skip lines</entry>
  
  <!-- FileHistory -->
  
  <entry key="LAST_OPENED">This memorizes the last files that have been opened by the program.</entry>
  
  <!-- FileSelector -->
  
  <entry key="FILE">file</entry>
  <entry key="DIRECTORY">directory</entry>
  <entry key="OPEN">open</entry>
  <entry key="SAVE">save</entry>
  <entry key="SELECT_TARGET_FILE">Select the file to be %s.</entry>
  <entry key="TO_BE_OPENED">opened</entry>
  <entry key="TO_BE_SAVED">saved</entry>
  <entry key="SELECT_TARGET_DIRECTORY">Select the default directory to %s various kinds of files.</entry>
  
  <!-- GUIOptions -->
  
  <entry key="CHECK_FOR_UPDATES">Decide whether or not this program should search for updates at start-up.</entry>
  <entry key="GUI">Can be used in combination with = true or = false or just --gui. Specifies whether or not a program should display its graphical user interface.</entry>
  <entry key="OPEN_DIR">Standard directory where user files can be found.</entry>
  <entry key="SAVE_DIR">Standard directory where the user may save some files.</entry>
  <entry key="DEFAULT_DIRECTORIES">Default directories</entry>
  <entry key="DEFAULT_DIRECTORIES_TOOLTIP">Define the default directories to open and save files. These directories will be used as the first search target when selecting files in this graphical user interface.</entry>
  
  <!-- GUITools -->
  
  <entry key="NO_READ_ACCESS_MESSAGE">Cannot read %s %s.</entry> <!-- first %s will be replaced by THE_FILE or THE_DIRECTORY and the second %s by the absolute path -->
  <entry key="NO_READ_ACCESS_TITLE">No reading access</entry>
  <entry key="NO_WRITE_ACCESS_MESSAGE">Cannot write to %s %s.</entry> <!-- first %s will be replaced by THE_FILE or THE_DIRECTORY and the second %s by the absolute path -->
  <entry key="NO_WRITE_ACCESS_TITLE">No writing access</entry>
  <entry key="OVERRIDE_EXISTING_FILE_QUESTION">%s %s already exists. Do you really want to overwrite it?</entry> <!-- first %s will be replaced by THE_FILE or THE_DIRECTORY and the second %s by the absolute path -->
  <entry key="OVERRIDE_EXISTING_FILE_TITLE">Overwrite existing file?</entry>
  <entry key="THE_DIRECTORY">the directory</entry>
  <entry key="THE_FILE">the file</entry>
  
  <!-- JColumnChooser -->
  
  <entry key="EG">E.g., </entry>
  <entry key="NOT_AVAILABLE">Not available</entry>
  <entry key="INVALID_COLUMN">Invalid column</entry>
  
  <!-- JHelpBrowser -->
  
  <entry key="ONLINE_HELP">Online Help</entry>
  <entry key="ONLINE_HELP_TOOLTIP">This is the main online help.</entry>
  <entry key="COMMAND_LINE_ARGUMENTS">Command-line arguments</entry>
  <entry key="LAST_PAGE">Last Page</entry>
  <entry key="NEXT">Next</entry>
  <entry key="NEXT_PAGE">Next Page</entry>
  <entry key="SAVE">Save</entry>
  <entry key="SAVE_TOOLTIP">Save the current page as HTML file.</entry>
  
  <!-- KeyProvider -->
  
  <entry key="ADDITIONAL_OPTIONS">Additional options</entry>
  <entry key="ALL_POSSIBLE_VALUES_FOR_TYPE">All possible values for type</entry>
  <entry key="ARE">are</entry>
  <entry key="AND">and</entry>
  <entry key="ARGS_MUST_FIT_INTO_RANGE">Arguments must fit into the range %s.</entry>
  <entry key="DEFAULT_VALUE">Default value</entry>
  
  <!-- MultiplePreferencesPanel -->
  
  <entry key="USER_PREFERENCES">User preferences</entry>
  
  <!-- Option -->
  
  <entry key="ACCEPTS">Accepts</entry>
    
  <!-- PreferencesDialog -->
  
  <entry key="APPLY">Apply;Persistently saves the current configuration but keeps the dialog open.</entry>
  <entry key="BROWSE">Browse</entry>
  <entry key="CANCEL">Cancel;Closes this dialog without saving, i.e., all preferences will get lost.</entry>
  <entry key="DEFAULTS">Defaults;Restores the default configuration, i.e., all current preferences will be replaced by the default values.</entry>
  <entry key="OK">OK;Saves the current configuration and closes this dialog.</entry>
  <entry key="PREFERENCES">Preferences</entry>
  
  <!-- SBFileFilter -->
  
  <entry key="IMAGE_FILES">Image files</entry>
  <entry key="TEXT_FILES">Text files</entry>
  <entry key="TeX_FILES">TeX files</entry>
  <entry key="SBML_FILES">SBML files</entry>
  <entry key="CSV_FILES">Comma separated files</entry>
  <entry key="JPEG_FILES">Joint Photographic Experts Group files</entry>
  <entry key="PNG_FILES">Portable Network Graphics files</entry>
  <entry key="PDF_FILES">Portable Document Format files</entry>
  <entry key="HTML_FILES">Hypertext markup language files</entry>
  <entry key="GRAPHML_FILES">GraphML files</entry>
  <entry key="GML_FILES">Graph Modeling Language files</entry>
  <entry key="GIF_FILES">Graphics Interchange Format files</entry>
  <entry key="YGF_FILES">Y Graph Format files</entry>
  <entry key="TGF_FILES">Trivial graph format files</entry>
  <entry key="DIRECTORIES_ONLY">Directories only</entry>
  
  
  <!-- SBAcceptAllFileFilter -->
  
  <entry key="ACCEPT_ALL_FILES">All files (*)</entry>
  
  <!-- UpdateMessage -->
  
  <entry key="SHOW_RELEASE_NOTES">show release notes</entry>
  <entry key="HIDE_RELEASE_NOTES">hide release notes</entry>
  <entry key="UPDATE_IS_AVAILABLE">An update for %s is available.</entry>
  <entry key="COMMAND_LINE_UPDATE_MESSAGE">\nUpdate notification:\n--------------------\nA new version of %s is available.\nPlease visit %s\nto obtain version %s.\nFor your information: you are now using %s version %s.\n</entry>
  <entry key="NO_UPDATE_AVAILABLE_FOR_CURRENT_VERSION_MESSAGE">You are already using the latest version %s of %s. No newer version could be found.</entry>
  <entry key="NO_UPDATE_AVAILABLE_FOR_CURRENT_VERSION_TITLE">No update available</entry> 
  
</properties>