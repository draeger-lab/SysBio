<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <comment>
	This configuration provides locale-dependent names for warnings
	and error messages in a (graphical) user interface.
  </comment>
  
  <!-- CSVReaderColumnCooser -->
  
  <entry key="INVALID_CSV-FILE">Invalid or empty CSV file.</entry>
  <entry key="NO_SUCH_COLUMN_CHOOSER">No column chooser named "%d".</entry>
  
  <!-- ImageTools -->
  
  <entry key="COULD_NOT_LOAD_IMAGE">"Could not load image %s.\n"</entry>
  <entry key="COULD_NOT_LOAD_ICONS_FROM_DIR">Could not load icons from directory %s.</entry>
  <entry key="WRONG_PERCENTAGE_IN_IMAGE">Wrong percentage in %s: %f.</entry>
  
  <!-- JBrowserPane -->
  
  <entry key="CANNOT_FOLLOW_LINK">Can't follow link to </entry>
  
  <!-- Option -->
  
  <entry key="VALUE_MUST_BE_POSITIVE">%s must be a positive number.</entry>
  
  <!-- Range -->
  
  <entry key="CLASS_NOT_COMPARABLE">Cannot compare class %s. Please implement the Comparable interface.</entry>
  <entry key="REQUIRED_TYPE_FOR_X_MUST_BE">When initialized with a %s, the required type must be an instance of %s.</entry>
  <entry key="RANGE_IN_WRONG_FORMAT">Range in wrong format: '%s'.</entry>
  
  <!-- SBFileFilter -->
  
  <entry key="FILE_TYPE_MUST_NOT_BE_UNDEFINED">FileType must not be UNDEFINED.</entry>
  
  <!-- SBPreferences -->
  
  <entry key="BACKING_STORE_EXCEPTION_MESSAGE">Could not persistently store the user configuration for %s.</entry> <!-- Requires a KeyProvider name as argument. -->
  <entry key="COULD_NOT_LOAD_PROPERTIES_MESSAGE">Could not load properties for %s.</entry> <!-- Requires a KeyProvider name as argument. -->
  <entry key="COULD_NOT_LOAD_PROPERTIES_FROM_FILE">Could not load properties for %s from configuration file %s.</entry> <!-- Requires name of a KeyProvider and absolute path to corresponding configuration file -->
  <entry key="INVALID_DATA_TYPE_FOR_OPTION">The value of option %s is of invalid type. Please specify an instance of %s.</entry> <!-- Requires a value and an Option name. -->
  <entry key="NO_DEFAULT_VALUE_FOR_OPTION_IN_FILE">No default value available for option %s in file %s.</entry> <!-- Requires an Option name and an absolute path to a file. -->
  <entry key="NO_SUCH_OPTION_DEFINED_BY_KEYPROVIDER">No such option %s defined by %s.</entry> <!-- Requires an Option name and a KeyProvider. -->
  <entry key="NO_VALUE_DEFINED_FOR_PROPERTY">No default value defined for property %s.</entry> <!-- Requires a wrongly used name of a property. -->
  <entry key="OPTION_OUT_OF_RANGE">Option %s is not in range.</entry> <!-- Requires name of an Option -->
  <entry key="RANGE_CONSTRAINT_VIOLATION_MESSAGE">The value "%s" for option "%s" is out of range. Please select a value that satisfies the following constraint: %s.</entry> <!-- Requires a value, an Option name, and a Range string. -->
  <entry key="REMOVING_INVALID_ENTRY">Removing invalid key-value pair %s:%s.</entry>
  <entry key="VALUE_FOR_OPTION_NOT_AVAILABLE">Could not determine value belonging to option %s.</entry> <!-- Requires the name of an Option. -->
  
</properties>
