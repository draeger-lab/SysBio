 *
 * This is a sample Gadget configuration file for Garuda. There are multiple
 * place holders in this file that need to be editeted in order to use this
 * file. You can add an arbitrarily long list of input and output file formats,
 * here is only one sample for each type. You can remove this as well. Also,
 * the list of screenshots does not necessarily have to contain three. You can
 * add as many as you like.
 *
 * Version: $Rev$
-->
<Gadget>
  <UUID>[GADGET_UUID]</UUID>
  <Name>[GADGET_NAME]</Name>
  <LaunchCommand>cmd /c start javaw.exe -jar "gadgets/[GADGET_UUID]/[JAR_NAME]"</LaunchCommand>
  <LaunchCommandWin>cmd /c start javaw.exe -jar "gadgets/[GADGET_UUID]/[JAR_NAME]"</LaunchCommandWin>
  <LaunchCommandMac>open "gadgets/[GADGET_UUID]/[JAR_NAME]"</LaunchCommandMac>
  <LaunchCommandLinux>java -jar "gadgets/[GADGET_UUID]/[JAR_NAME]"</LaunchCommandLinux>
  <IconPaths>
    <Icon32Path>icons/[ICON_NAME]</Icon32Path>
  </IconPaths>
  <Snapshots>
    <SnapshotPath>snapshot/[SCREENSHOT_1]</SnapshotPath>
    <SnapshotPath>snapshot/[SCREENSHOT_2]</SnapshotPath>
    <SnapshotPath>snapshot/[SCREENSHOT_3]</SnapshotPath>
  </Snapshots>
  <Categories>
    <Category>[CATEGORY_1]</Category>
    <Category>[CATEGORY_2]</Category>
  </Categories>
  <Provider>University of Tuebingen</Provider>
  <Description>[DESCRIPTION]</Description>
  <LoadableKeys>
    <InputFileFormat>
      <FileExtension>xml</FileExtension>
      <FileFormat>SBML</FileFormat>
      </InputFileFormat>
    <!--
    <InputFileFormat>
      <FileExtension>sbml</FileExtension>
      <FileFormat>SBML</FileFormat>
    </InputFileFormat>
    <InputFileFormat>
      <FileExtension>xml</FileExtension>
      <FileFormat>sbml</FileFormat>
      </InputFileFormat>
    <InputFileFormat>
      <FileExtension>sbml</FileExtension>
      <FileFormat>sbml</FileFormat>
    </InputFileFormat>
    -->
    <OutputFileFormat>
      <FileExtension>xml</FileExtension>
      <FileFormat>SBML</FileFormat>
    </OutputFileFormat>
    <!--
    <OutputFileFormat>
      <FileExtension>sbml</FileExtension>
      <FileFormat>SBML</FileFormat>
    </OutputFileFormat>
    <OutputFileFormat>
      <FileExtension>xml</FileExtension>
      <FileFormat>sbml</FileFormat>
    </OutputFileFormat>
    <OutputFileFormat>
      <FileExtension>sbml</FileExtension>
      <FileFormat>sbml</FileFormat>
    </OutputFileFormat>
    -->
  </LoadableKeys>
  <LaunchEnv>
    <Id>f22aa6a2-beff-4487-b838-03b8275a4361</Id>
  </LaunchEnv>
</Gadget>
